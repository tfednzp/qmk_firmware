    case KC_A ... KC_Z:
      if (record->event.pressed) {
#if defined(UNICODE_ENABLE) || defined(UNICODEMAP_ENABLE)
        user_mod_state = get_mods() & MOD_MASK_ALT;
        if (user_mod_state) {
          unregister_mods(MOD_BIT(KC_RALT));
          send_unicode_hex_string("00AE");
          set_mods(user_mod_state);
          return false;
        }
#endif
      }
      break;

